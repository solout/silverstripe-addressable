{
    "name": "symbiote/silverstripe-addressable",
    "description": "SilverStripe addressable and geocoding module",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "symbiote",
        "addressable",
        "geocoding"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "nyeholt@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^3.1",
        "guzzlehttp/guzzle": "^5.3.1|^6.2.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "phpcbf": "phpcbf code/ tests/",
        "phpcs": "phpcs code/ tests/"
    },
    "extra": {
        "installer-name": "addressable",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "replace": {
        "ajshort/silverstripe-addressable": "self.version",
        "silverstripe-australia/addressable": "self.version"
    }
}
